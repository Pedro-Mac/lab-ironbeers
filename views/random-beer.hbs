<div class="a">
  <img src="{{ randomBeer.image_url }}" alt="{{ randomBeer.name }}" class="custom-image">
  <div>
    <h2>{{ randomBeer.name }}</h2>
    <p>{{ randomBeer.tagline }}</p>

    <div>
      <h3>Food Pairing</h3>
      <ul>
        {{#each randomBeer.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
      </ul>
    </div>

    <div>
      <p><strong>Brewer's tipes:</strong> {{ randomBeer.brewers_tips }}</p>
    </div>

  </div>
</div>